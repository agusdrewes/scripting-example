{
	"name": "nodejs-postgres-sequelize",
	"version": "1.0.0",
	"description": "A Node.js project using TypeScript and Sequelize to connect to a PostgreSQL database.",
	"main": "src/app.ts",
	"scripts": {
		"start": "ts-node src/app.ts",
		"build": "tsc",
		"dev": "ts-node-dev src/app.ts",
		"db:migrate": "npx sequelize db:migrate --env development",
		"db:down:all": "npx sequelize db:migrate:undo:all"
	},
	"dependencies": {
		"body-parser": "^2.2.0",
		"express": "^4.17.1",
		"pg": "^8.14.1",
		"pg-hstore": "^2.3.4",
		"sequelize": "^6.6.5"
	},
	"devDependencies": {
		"@types/express": "^4.17.13",
		"@types/node": "^16.11.7",
		"sequelize-cli": "^6.6.2",
		"ts-node": "^10.4.0",
		"ts-node-dev": "^1.1.8",
		"typescript": "^4.4.4"
	},
	"author": "",
	"license": "ISC"
}
